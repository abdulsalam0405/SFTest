<ERPS>
<ERP NAME="Sage100ERP">
<PARAMS>
<PARAM TYPE="TEXT" LENGTH="200" OPTIONS="" CODE="InstallationPath" REQUIRED="TRUE" WIDTH="400">Sage 100 Home Directory</PARAM>
<PARAM TYPE="TEXT" LENGTH="3" OPTIONS="" CODE="CompanyCode" REQUIRED="TRUE" WIDTH="150">Company Code</PARAM>
<PARAM TYPE="TEXT" LENGTH="20" OPTIONS="" CODE="Sage100Server" REQUIRED="TRUE" WIDTH="150">Sage 100 Server Name</PARAM>
<PARAM TYPE="TEXT" LENGTH="20" OPTIONS="" CODE="Username" REQUIRED="TRUE" WIDTH="150">Sage 100 Username</PARAM>
<PARAM TYPE="PASSWORD" LENGTH="20" OPTIONS="" CODE="Password" REQUIRED="TRUE" WIDTH="150">Sage 100 Password</PARAM>
<PARAM TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="SQLServer" WIDTH="150">SQL Server Name</PARAM>
<PARAM TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="SQLCompanyInstance" WIDTH="150">SQL Company Instance Name</PARAM>
<PARAM TYPE="TEXT" LENGTH="100" OPTIONS="" CODE="WindowsDomain" REQUIRED="TRUE" WIDTH="150">Windows Domain Name</PARAM>
<PARAM TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="WindowsUsername" REQUIRED="TRUE" WIDTH="150">Windows Username</PARAM>
<PARAM TYPE="PASSWORD" LENGTH="50" OPTIONS="" CODE="WindowsPassword" REQUIRED="TRUE" WIDTH="150">Windows Password</PARAM>
</PARAMS>
<IMPORTOPTIONS>
<IMPORTOPTION NAME="Import Customers" VALUE="Customer" TYPE="FILTER">
<CRITERIAGROUPS>
<CRITERIAGROUP NAME="CustUpdateDate" LABEL="Customer Last Sync">
<CRITERIA TYPE="DATE" LENGTH="50" OPTIONS="" CODE="ServerName" WIDTH="150">Customer Last Sync</CRITERIA>
</CRITERIAGROUP>
<CRITERIAGROUP NAME="CustomerNumber" LABEL="Customer Number">
<CRITERIA TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="ServerName" WIDTH="150">From Customer No.</CRITERIA>
<CRITERIA TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="ServerName" WIDTH="150">To Customer No.</CRITERIA>
</CRITERIAGROUP>
</CRITERIAGROUPS>
</IMPORTOPTION>
<IMPORTOPTION NAME="Import Products" VALUE="Product" TYPE="FILTER">
<CRITERIAGROUPS>
<CRITERIAGROUP NAME="ProdUpdateDate" LABEL="Product Last Sync">
<CRITERIA TYPE="DATE" LENGTH="50" OPTIONS="" CODE="ProdUpdateDate" WIDTH="150">Product Last Sync</CRITERIA>
</CRITERIAGROUP>
<CRITERIAGROUP NAME="ProductCode" LABEL="Product Code">
<CRITERIA TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="FromProductCode" WIDTH="150">From Product Code</CRITERIA>
<CRITERIA TYPE="TEXT" LENGTH="50" OPTIONS="" CODE="ToProductCode" WIDTH="150">To Product Code</CRITERIA>
</CRITERIAGROUP>
</CRITERIAGROUPS>
</IMPORTOPTION>
<IMPORTOPTION NAME="Synchronize Dropdowns" VALUE="Dropdown" TYPE="SYNC" HASFILTEROPTION="false">
<CRITERIAGROUPS>
<CRITERIAGROUP NAME="" LABEL="">
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Terms_Code" WIDTH="">Terms Code</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Credit_Card_Type" WIDTH="">Credit Card Type</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Payment_Type" WIDTH="">Payment Type</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Warehouse" WIDTH="">Warehouse</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Tax_Schedule" WIDTH="">Tax Schedule</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Ship_Method" WIDTH="">Ship Method</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="AR_Division" WIDTH="">AR Division</CRITERIA>
<CRITERIA TYPE="CHECKBOX" LENGTH="" OPTIONS="" CODE="Sales_Person" WIDTH="">Sales Person</CRITERIA>
</CRITERIAGROUP>
</CRITERIAGROUPS>
</IMPORTOPTION>
</IMPORTOPTIONS>
</ERP>
<ERP NAME="SageERPX3">
	<PARAMS>
		<PARAM TYPE="TEXT" LENGTH="200" OPTIONS="" CODE="poolAlias" REQUIRED="TRUE" WIDTH="400">Pool Name</PARAM>
		<PARAM TYPE="TEXT" LENGTH="100" OPTIONS="" CODE="codeUser" REQUIRED="TRUE" WIDTH="150">Username</PARAM>
		<PARAM TYPE="PASSWORD" LENGTH="20" OPTIONS="" CODE="password" REQUIRED="FALSE" WIDTH="150">Password</PARAM>
		<PARAM TYPE="TEXT" LENGTH="20" OPTIONS="" CODE="poolId" REQUIRED="TRUE" WIDTH="150">X3 Folder Name</PARAM>
		<PARAM TYPE="TEXT" LENGTH="3" OPTIONS="" CODE="codeLang" REQUIRED="TRUE" WIDTH="150">Language</PARAM>
		<PARAM TYPE="TEXT" LENGTH="3" OPTIONS="" CODE="version" REQUIRED="TRUE" WIDTH="150">Version</PARAM>
	</PARAMS>
</ERP>
<ERP NAME="Sage300ERP">
<PARAMS>
<PARAM TYPE="TEXT" LENGTH="6" OPTIONS="" CODE="CompanyCode" REQUIRED="TRUE" WIDTH="150">Company Code</PARAM>
<PARAM TYPE="TEXT" LENGTH="100" OPTIONS="" CODE="Username" REQUIRED="TRUE" WIDTH="150">Username</PARAM>
<PARAM TYPE="PASSWORD" LENGTH="20" OPTIONS="" CODE="Password" REQUIRED="FALSE" WIDTH="150">Password</PARAM>
</PARAMS>
</ERP>
</ERPS>