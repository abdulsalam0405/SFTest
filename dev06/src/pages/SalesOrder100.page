        case "Warning":
            j$('#imgWarning').show();
            break;
        case "Confirm":
            j$('#imgConfirm').show();
            break;
        case "Info":
            j$('#imgInfo').show();
            break;
    }

    j$('#msgHeader').show();
    j$('#messages').html(Message);

    j$('#msgHeader').fadeIn();
    j$('#msgHeader').fadeOut(5000,"linear"); 
}

