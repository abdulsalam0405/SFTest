       function fncCalculateItemPrice()
        {
            debugger;
            index = gblIndex;
            var Discount = j$('#Discount').val() != '' ? parseFloat(j$('#Discount').val()).toFixed(2) : 0.00; 
            var Quantity = j$('#Quantity').val() != '' ? parseFloat(j$('#Quantity').val()).toFixed(2) : 0.00; 
            var QuotedPrice = j$('#QuotedPrice').val() != '' ? parseFloat(j$('#QuotedPrice').val()).toFixed(2) : 0.00;
            var UOMConversionFactor = j$('#UOMConversionFactor').val() != '' ? parseFloat(j$('#UOMConversionFactor').val()).toFixed(2) : 1.00;
           // alert(j$('#ProductUnitPrice_'+index).val());
            var UnitPrice = j$('#ProductUnitPrice_'+index).val()!= '' ? parseFloat(j$('#ProductUnitPrice_'+index).val()).toFixed(2) : 0.00;
            var QuantityOrdered = j$('#ProductQuantity_'+index).val()!= '' ? parseFloat(j$('#ProductQuantity_'+index).val()).toFixed(2) : 0.00;
           
            var ExtensionAmt = UnitPrice * QuantityOrdered ; 
               
            j$('#ProductExtAmt_'+index).val(ExtensionAmt); 
              
            var ExtendedPrice;
            //var ExtendedPrice1=j$('#ProductExtAmt_'+indexPosition).val();
   
             QuotedPrice = j$('#QuotedPrice').val() != '' ? parseFloat(j$('#QuotedPrice').val()).toFixed(2) : 0.00; 
            
            if(Quantity != NaN && QuotedPrice != NaN)
            {
                 ExtendedPrice = QuotedPrice * Quantity * UOMConversionFactor;
                 
                  if(Discount != NaN)
                  {
                       ExtendedPrice -= ExtendedPrice * (Discount / 100);
                       j$('#ExtendedPrice').val(parseFloat(ExtendedPrice ).toFixed(2));                   
                  }                                   
            }
        }
        
